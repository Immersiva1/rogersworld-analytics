{
  "name": "rogersworld-analytics",
  "files": [
    "dist"
  ],
  "main": "./dist/analytics.umd.js",
  "module": "./dist/analytics.es.js",
  "exports": {
    ".": {
      "import": "./dist/analytics.es.js",
      "require": "./dist/analytics.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run docs && vite build --config vite-lib.config.js && mv dist dist-lib && vite build && mv dist-lib/* dist/ && rm -rf dist-lib",
    "serve": "vite preview",
    "docs": "ts-node scripts/docs.md > README.md"
  },
  "devDependencies": {
    "@types/node": "^16.7.1",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5",
    "vite": "^2.5.0"
  }
}
